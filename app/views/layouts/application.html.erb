<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title yield(:title) %></title>
    <%= render "layouts/rails_layout" %>
  </head>
  <body>
    <%= render "layouts/header" %>
    <%= link_to t(".language_en"), locale: "en" %>
    <%= link_to t(".language_vi"), locale: "vi" %>
    <div class="container">
      <%= yield %>
      <%= render "layouts/footer" %>
    </div>
  </body>
</html>
